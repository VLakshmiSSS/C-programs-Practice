#include<stdio.h>

int main()
{
    int n=5,i,j,k,key,a[5]={64,25,12,22,11};
    
    //sorting algorithm
    for(i=1;i<n;i++)
    {
        key=a[i];
        for(j=i-1;j<0;j--)
        {
            if(key<a[j-1])
        }
    }
    
    //print sorted array
    printf("\nThe sorted array is:\n");
    for(k=0;k<n;k++)
    printf("%d\n",a[k]);
    
    
    return(0);
}
