#include<stdio.h>

int main()
{
    int n=5,i,j,k,a[5]={64,25,12,22,11};
    
    for(i=0;i<n;i++)
    {
        for(j=0;j<n-1;j++)
        {
            if(a[j]>a[j+1])
            {
                int temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
    printf("\nThe sorted array is:\n");
    for(k=0;k<n;k++)
    printf("%d\n",a[k]);
    
    
    return(0);
}
