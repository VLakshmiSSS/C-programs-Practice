// Online C compiler to run C program online
#include <stdio.h>

int main() 
{
    int n=5;
    int a[5]={64,25,12,22,11};
    int i,k;
    for(i=0;i<n;i++)
    {
        for(k=i+1;k<n;k++)
        {
            if(a[k]<a[i])
            {
                int temp=a[k];
                a[k]=a[i];
                a[i]=temp;
            }
        }
    }
    printf("\nSorted array is:\n");
    for(i=0;i<n;i++)
    printf("%d\n",a[i]);

    return 0;
}
